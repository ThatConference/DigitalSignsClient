{
  "hosting": {
    "public": "build",
    "headers": [{ "source": "/service-worker.js", "headers": [{ "key": "Cache-Control", "value": "no-cache" }] }],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**", "**/docs/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
